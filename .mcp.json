{
  "mcpServers": {
    "angelamcp": {
      "command": "python",
      "args": ["-m", "src.mcp_server"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}",
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
        "DATABASE_URL": "${DATABASE_URL}",
        "REDIS_URL": "${REDIS_URL}",
        "APP_ENV": "production"
      }
    }
  }
}
